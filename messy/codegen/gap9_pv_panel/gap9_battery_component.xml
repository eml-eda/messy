            </ipxact:wire>
        </ipxact:port>
        <ipxact:port>
            <ipxact:name>V</ipxact:name>
            <ipxact:transactional>
                <ipxact:initiative>provides</ipxact:initiative>
                <ipxact:transTypeDefs>
                    <ipxact:transTypeDef>
                        <ipxact:typeName>double</ipxact:typeName>
